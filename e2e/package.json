{
  "name": "lambda-at-home-e2e-tests",
  "version": "0.3.0",
  "description": "End-to-end test suite for Lambda@Home using Node.js native test runner",
  "main": "tests/setup.js",
  "scripts": {
    "test": "node --test --test-concurrency=1 tests/integration/*.test.js",
    "test:managed": "node --test --test-concurrency=1 tests/integration/*.test.js",
    "test:external": "SKIP_SERVICE_START=1 node --test --test-concurrency=1 tests/integration/*.test.js",
    "test:watch": "SKIP_SERVICE_START=1 node --test --watch tests/integration/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/integration/*.test.js",
    "test:performance": "SKIP_SERVICE_START=1 node --test tests/integration/performance.test.js",
    "test:service": "SKIP_SERVICE_START=1 node --test tests/integration/service.test.js",
    "test:runtimes": "SKIP_SERVICE_START=1 node --test tests/integration/runtimes.test.js",
    "test:integration": "node --test --test-concurrency=1 tests/integration/*.test.js",
    "cleanup": "node utils/cleanup.js"
  },
  "keywords": [
    "lambda",
    "test",
    "node:test",
    "lambda-at-home",
    "integration"
  ],
  "author": "Lambda@Home",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "axios": "^1.6.0"
  }
}
